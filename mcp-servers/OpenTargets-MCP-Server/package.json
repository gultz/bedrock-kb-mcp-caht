{
  "name": "opentargets-server",
  "version": "1.0.0",
  "description": "Production-ready MCP server for Open Targets gene-drug-disease associations",
  "main": "build/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc && node -e \"require('fs').chmodSync('build/index.js', '755')\"",
