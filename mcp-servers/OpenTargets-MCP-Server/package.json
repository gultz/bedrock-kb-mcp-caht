    "disease-associations",
    "drug-targets",
    "clinical-trials",
    "evidence-based",
    "ontology",
    "chembl",
    "ensembl",
    "efo",
    "pathway-analysis",
    "safety-assessment",
    "drug-repurposing"
  ],
  "author": "Augmented Nature",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "bin": {
    "opentargets-server": "./build/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/augmented-nature/opentargets-mcp-server"
  },
  "homepage": "https://github.com/augmented-nature/opentargets-mcp-server#readme",
